              <gmd:DQ_QuantitativeResult>
                <gmd:value>
                  <gco:Record xmlns:gco="http://www.isotc211.org/2005/gco">0.236 feet RMSE X; 0.212 feet RMSE Y</gco:Record>
                </gmd:value>
              </gmd:DQ_QuantitativeResult>
            </gmd:result>
          </gmd:DQ_AbsoluteExternalPositionalAccuracy>
